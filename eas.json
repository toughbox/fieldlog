{
  "cli": {
    "version": ">= 3.10.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "distribution": "internal",
        "withoutCredentials": true,
        "autoIncrement": true
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "distribution": "store",
        "withoutCredentials": false,
        "autoIncrement": true
      }
    }
  }
}
